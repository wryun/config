" Defaults
set nocompatible
set backspace=indent,eol,start
set autoindent
set ruler
set showcmd
syntax enable

" James
"set ts=4 sw=4 sts=4
set ts=8 sw=4 sts=4
"set ts=8 sw=2 sts=2
set expandtab
set display+=lastline
set linebreak
"map j gj
"map k gk
set nobackup
set viminfo=
"color elflord
"set foldmethod=indent foldnestmax=2 fml=3
"highlight Folded ctermfg=63 ctermbg=4 guibg=blue guifg=white
"highlight FoldColumn ctermfg=63 ctermbg=4 guibg=blue guifg=white
